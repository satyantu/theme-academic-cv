	keywords = {★, LCA, Carbon dioxide utilisation, Carbon Neutral, Catalytic Hydrothermal Gasification, Power-to-Gas, VRE storage},
	pages = {115923},
	file = {fozer2020.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\R69NS26N\\fozer2020.pdf:application/pdf},
}

