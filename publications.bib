@article{zhao2023,
	title = {A novel technology for phosphorus recovery from leaching residue of montebrasite by alkaline leaching and crystallization},
	volume = {415},
	issn = {09596526},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S095965262301990X},
	doi = {10.1016/j.jclepro.2023.137832},
	abstract = {Phosphate rock, a finite and non-renewable resource on earth, is facing the dilemma of resource depletion and quality decline, thus it is urgent to fully utilize any potential phosphorus resources. In this paper, a novel process combined alkaline leaching and crystallization for phosphorus recovery from the leaching residue of monte­ brasite (LRM) was proposed. The main component of LRM was determined to be AlPO4. Thermodynamic study of the Al3+-PO43--H2O system was carried out, which revealed that the complete dissolution region of AlPO4 were pH {\textless} 0.8 and pH {\textgreater} 12.2. Using NaOH as the leaching agent, a series of single factor experiments were conducted to determine the optimal leaching parameters. The results demonstrated that the extraction rates of P, Al and Si were 89.40\%, 86.49\% and 23.33\%, respectively, at leaching temperature of 60 ◦C, leaching time of 1 h, liquid-tosolid ratio of 12 mL/g and NaOH concentration of 120 g/L. Moreover, phosphate in leach liquor was recovered in the form of Na3PO4 by crystallization. The recovery rate of P in leach liquor reached 95.72\% at 5 ◦C for standing 24 h, whereas only 6.51\% of Al was lost. The process proposed in this study provides the possibility of extracting phosphorus from a new potential phosphorus resource.},
	language = {en},
	urldate = {2023-08-25},
	journal = {Journal of Cleaner Production},
	author = {Zhao, Qinqing and Ma, Baozhong and Lv, Yingwei and Hu, Dezhi and Chen, Yongqiang and Wang, Chengyan},
