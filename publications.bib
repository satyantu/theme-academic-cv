	month = apr,
	year = {2023},
	keywords = {Sustainability, Resource recovery, Biological macromolecules, Energy, renewable, Nanobiocatalyst},
	pages = {124585},
	file = {ghalkhani2023.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\PISWCIQ4\\ghalkhani2023.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Satyabrat\\Zotero\\storage\\BTLUHPWE\\S0141813023014794.html:text/html},
