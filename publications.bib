	keywords = {Resource utilization, Sewage sludge (SS), Chemical looping gasification (CLG), Copper slag, Oxygen carrier (OC)},
	pages = {17823--17834},
	file = {deng2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\BVK4YEQ9\\deng2019.pdf:application/pdf},
}

@article{zheng2019,
	title = {Thermodynamic investigation on gasification performance of sewage sludge-derived hydrochar: {Effect} of hydrothermal carbonization},
	volume = {44},
	issn = {03603199},
	url = {https://doi.org/10.1016/j.ijhydene.2019.02.200},
	doi = {10.1016/j.ijhydene.2019.02.200},
	abstract = {Compared with the conventional thermal drying process, hydrothermal carbonization (HTC) can reduce the energy cost of water removal from sewage sludge prior to its steam gasification. However, less attention is paid on the interactions between HTC and gasification. In this study, the thermodynamic evaluation on hydrochar gasification performance under different operating conditions including HTC duration (τ), HTC temperature (THTC), gasification temperature (Tg), and steam/hydrochar mass ratio (S/C ratio) is performed. Two indicators including carbon conversion rate (CC) and cold gas efficiency (CGE) are used to assess the gasification performance. The results show that elevating both gasification temperature and S/C ratio can enhance the H2 production, which also result in the increase of CC and CGE. The content and gasification activity of fixed carbon increase under moderate HTC duration and temperature, favoring the H2 formation despite of the apparent loss of volatiles species in the hydrochar. Longer HTC duration or higher HTC temperature declines the H2 production due to the sharp reduction of carboxyl and hydroxyl groups, weakening water gas reaction and on-site reforming reaction of tar occurred on the hydrochar surface. In terms of the values of CC = 93.9\% and CGE = 64.38\%, the optimum HTC conditions of τ = 30min and THTC = 200 °C can be determined. The data provided here favor guiding HTC treatment of sewage sludge targeting gasification and thus promoting the development of this promising waste-to-energy technology.},
	number = {21},
	journal = {International Journal of Hydrogen Energy},
	author = {Zheng, Xiaoyuan and Chen, Wei and Ying, Zhi and Huang, Jin and Ji, Shasha and Wang, Bo},
	year = {2019},
	note = {Publisher: Elsevier Ltd},
	keywords = {Sewage sludge, Hydrothermal carbonization, Steam gasification, Hydrochar, Aspen plus},
	pages = {10374--10383},
	file = {zheng2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\HSDUFLAA\\zheng2019.pdf:application/pdf},
}

@article{zhao2019a,
