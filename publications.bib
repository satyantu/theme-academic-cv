	note = {Publisher: Elsevier Ltd},
	keywords = {Sewage sludge, Phosphorous recovery, Calcium silicate hydrates, Release properties},
	pages = {1087--1093},
	file = {lee2018.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\KPUUL7T6\\lee2018.pdf:application/pdf},
}

