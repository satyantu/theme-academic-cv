	author = {Ahmed, Warish and Bertsch, Paul M. and Bibby, Kyle and Haramoto, Eiji and Hewitt, Joanne and Huygens, Flavia and Gyawali, Pradip and Korajkic, Asja and Riddell, Shane and Sherchan, Samendra P. and Simpson, Stuart L. and Sirikanchana, Kwanrawee and Symonds, Erin M. and Verhagen, Rory and Vasan, Seshadri S. and Kitajima, Masaaki and Bivins, Aaron},
	month = dec,
