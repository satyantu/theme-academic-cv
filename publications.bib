@article{wang2020d,
	title = {Distribution and transformation behaviors of heavy metals and phosphorus during hydrothermal carbonization of sewage sludge},
	volume = {27},
	copyright = {2020 Springer-Verlag GmbH Germany, part of Springer Nature},
	issn = {1614-7499},
	url = {https://link.springer.com.remotexs.ntu.edu.sg/article/10.1007/s11356-020-08098-4},
	doi = {10.1007/s11356-020-08098-4},
	abstract = {This study investigated the distribution and transformation behaviors of heavy metals (HMs) and phosphorus (P) during hydrothermal carbonization (HTC) of sewage sludge. In addition to a large reduction in sludge volume, HTC significantly decreased the exchangeable fraction of HMs (Zn, Cu, Cr, Ni, and Mn) and increased their residual fraction, which resulted in immobilization of HMs accumulated in hydrochar. The ecological toxicity of HMs was greatly reduced, and all HMs exhibited their lowest risk levels after HTC at 220 °C for 1 h in 2\% H2SO4 solution. Most of the P ({\textasciitilde} 97\%) in the input sludge remained in the hydrochar after HTC. HTC facilitated transformation of organic P to inorganic P and promoted conversion of apatite P to non-apatite inorganic P under acidic conditions. The feasibility of recovering P from sludge by HTC was verified by an acid extraction experiment utilizing hydrochar, which recovered more than 90\% P.},
	language = {en},
