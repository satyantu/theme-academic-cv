	keywords = {Hydrogen, Biomass, Tar, Active carbon, Power, Two-stage gasification},
	pages = {106240},
	file = {jeong2020.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\TCSDDH5M\\jeong2020.pdf:application/pdf;jeong2020.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\B2R3J7YN\\jeong2020.pdf:application/pdf},
}

@article{yang2021,
