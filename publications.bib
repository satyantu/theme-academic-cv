}

@article{wisawapipat2017,
	title = {Solid-phase speciation and solubility of phosphorus in an acid sulfate paddy soil during soil reduction and reoxidation as affected by oil palm ash and biochar},
	volume = {65},
	issn = {15205118},
	url = {https://pubs.acs.org.remotexs.ntu.edu.sg/doi/full/10.1021/acs.jafc.6b03925},
	doi = {10.1021/ACS.JAFC.6B03925/SUPPL_FILE/JF6B03925_SI_001.PDF},
	abstract = {Understanding phosphorus (P) speciation and how redox conditions control P solubility in acid sulfate paddy soils with limited P availability is crucial for improving soil P availability. We examined P speciation and extractability in an acid sulfate paddy soil incorporated with oil palm ash (OPA) and biochar (OPB) during soil reduction and subsequent oxidation. Phosphorus K-edge X-ray absorption near edge structure (XANES) spectra of the soil samples revealed that P in the soil mainly occurred as P adsorbed to ferrihydrite and P adsorbed to gibbsite. During soil reduction, gibbsite-bound P was transformed into variscite, which was back-transformed to gibbsite-bound P during soil reoxidation. Sequential extraction results confirmed the dominance of Fe/Al (hydr)oxides-bound P (average 72\%) in the soils. The OPA incorporation increased the exchangeable P pool concurring with the decrease in gibbsite-bound P. The OPB incorporation enhanced the dissolved P from the residual pool presumably due to electron shuttling of biochar with Fe(III) minerals during soil reduction. Our results highlight P dynamics in paddy soils, which are of immense importance for effective P-management strategies in rice cultivation.},
	number = {4},
	urldate = {2021-12-15},
	journal = {Journal of Agricultural and Food Chemistry},
	author = {Wisawapipat, Worachart and Charoensri, Kamolchanok and Runglerttrakoolchai, Jirawat},
	month = feb,
	year = {2017},
	pmid = {28060497},
	note = {Publisher: American Chemical Society},
	keywords = {XANES, Phosphorus speciation, Redox, Rice, Sequential extraction, XAS},
	pages = {704--710},
	file = {wisawapipat2017.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\KQ3FNP5R\\wisawapipat2017.pdf:application/pdf},
}

