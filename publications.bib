	keywords = {Biogas, Microbial community, Pretreatment, Lignocellulose, Biodegradation, Microaerobic},
	file = {Amin2017.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\XSLCFLHE\\Amin2017.pdf:application/pdf},
}

