@article{saoudi2022a,
	title = {Mechanisms governing the dissolution of phosphorus and iron in sewage sludge by the bioacidification process and its correlation with iron phosphate speciation},
	volume = {307},
	issn = {0045-6535},
	url = {https://www.sciencedirect.com/science/article/pii/S004565352202197X},
	doi = {10.1016/j.chemosphere.2022.135704},
	abstract = {In the last two decades, phosphorus (P) recovery from sewage sludge liquors gained much interest for its high-quality product potential. However, the consistently reported constraints are the low phosphorus availability and the technical-economical difficulties to increase it through chemical acidification. This article discusses the mechanisms of phosphorus dissolution by the biological acidification process (Biological acidification or acidic fermentation) as an alternative to chemical acidification. In addition, we investigate the potential correlation between the phosphorus dissolution and iron phosphate speciation of several types of sludge from different sewage treatment plants and P removal technologies. The results show that the percentage of P dissolution by bioacidification is always higher than the P dissolution by chemical acidification at equal pH for all types of sludge except for the settled primary sludge. The highest P dissolution was recorded for the sludge from the Enhanced Biological P Removal process assisted with Chemical P Removal process (EBPR-CPR) with around 65\% of P dissolution. Three mechanisms were identified as contributing to the increased P dissolution by bioacidification: P release by the Polyphosphate Accumulating Organisms (PAO), P dissolution by pH decrease, and P dissolution by a biological activity at acidic pH (3.7–4) that includes iron reduction and aluminum dissolution. The principal component analysis and Pearson's correlation indicate that P dissolution by bioacidification is negatively correlated with the P-bound to ferric iron, hence positively correlated with the P-bound to ferrous iron, which characterizes the sludge from the EBPR-CPR process. This study suggests that the choice of the P removal technology significantly influences the P recovery from sewage sludge liquors.},
	language = {en},
	urldate = {2023-01-03},
	journal = {Chemosphere},
	author = {Saoudi, Mohamed Amine and Dabert, Patrick and Vedrenne, Fabien and Daumer, Marie-Line},
	month = nov,
	year = {2022},
	keywords = {Phosphorus recovery, Waste activated sludge, Phosphorus removal, Biological acidification, Iron phosphate speciation},
	pages = {135704},
	file = {saoudi2022a.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\JBDD8L4E\\saoudi2022a.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Satyabrat\\Zotero\\storage\\33DNWZYJ\\S004565352202197X.html:text/html},
}

