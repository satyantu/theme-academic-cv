}

@article{chawla2009,
	title = {A novel low-friction surface for biomedical applications: {Modification} of poly(dimethylsiloxane) ({PDMS}) with polyethylene glycol({PEG})-{DOPA}-lysine},
	volume = {90A},
	issn = {15493296},
