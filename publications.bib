	urldate = {2020-12-15},
	journal = {medRxiv},
	author = {Vallejo, Juan and Rumbo-Feal, Soraya and Conde-Pérez, Kelly and López-Oriona, Ángel and Tarrío-Saavedra, Javier and Reif, Rubén and Ladra, Susana and Rodiño-Janeiro, Bruno and Nasser, Mohammed and Cid, Ángeles and Veiga, María and Acevedo, Antón and Lamora, Carlos and Bou, Germán and Cao, Ricardo and Poza, Margarita},
