	author = {Liu, Fanghua and Rotaru, Amelia Elena and Shrestha, Pravin M. and Malvankar, Nikhil S. and Nevin, Kelly P. and Lovley, Derek R.},
	month = oct,
	year = {2012},
	note = {Publisher: The Royal Society of Chemistry},
	pages = {8982--8989},
	file = {Liu2012.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\5HXIEEQN\\Liu2012.pdf:application/pdf},
}

