	author = {Urych, Beata and Smoliński, Adam},
	year = {2021},
	keywords = {Sewage sludge, Biowaste, Biomass, sewage, sludge, Co-gasification, gasification, Co-pyrolysis, air, Kinetics study, nitrogen},
	file = {urych2021.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\3EGY2ZE2\\urych2021.pdf:application/pdf},
}

@article{barca2019,
