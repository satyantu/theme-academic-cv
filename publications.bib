	keywords = {Review, analytical hierarchy process, biogenic waste, hydrothermal processes, MCA, Multi criteria analysis, multi-criteria analysis, technique for order preference by similarity to id},
	pages = {912--923},
	file = {reissmann2018.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\Q5JXG9GK\\reissmann2018.pdf:application/pdf},
