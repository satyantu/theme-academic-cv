	keywords = {phosphate, speciation, nutrient cycling, soil chemistry, spectroscopy},
	pages = {43--88},
	file = {kruse2015.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\GRHN2DTY\\kruse2015.pdf:application/pdf;Snapshot:C\:\\Users\\Satyabrat\\Zotero\\storage\\FZ5UW7C4\\jpln.html:text/html},
