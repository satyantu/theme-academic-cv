	note = {Publisher: Pergamon},
	keywords = {Municipal solid wastes, Modeling, Calorific value, Elemental content, Regression},
	pages = {372--381},
	file = {komilis2012.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\VNURN7XD\\komilis2012.pdf:application/pdf},
}

@article{consonni2012,
