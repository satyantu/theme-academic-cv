	volume = {236},
	issn = {00456535},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0045653519320806},
	doi = {10.1016/j.chemosphere.2019.124841},
	abstract = {Hydrochars derived from municipal sewage sludge was analyzed for its feasibility for value-added recycling. Results of carbon content and elemental composition suggested that the hydrochars might not be comparable with pyrochars regarding to the carbon sequestration, long-term stability and fuel quality. Application as soil amendment would be a better approach for hydrochar utilization. To examine the potential benefits and risks of that, the total and available content of nutrients (i.e. N, P, and K) were measured, and the potentially toxic elements (PTEs, i.e. Zn, Cu, Pb, Ni and Cd) were analyzed for the total content, speciation, and leaching potential. Compared with pyrochars derived from the same feedstock, hydrochars had lower pH and higher cation exchange capacity. The available content of N (1.58–6.87 g/kg), P (0.270–0.901 g/kg), and K (0–0.873 g/kg) in the chars was less than the feedstock sludge (3.33 g/kg N, 3.02 g/kg P, 2.07 g/kg K), but still far higher than that of the agricultural soil (i.e. 0.014–0.488 g/kg N, 0.02 g/kg P, {\textless}0.1–0.272 g/kg). Remarkably, hydrochars showed better nutritional balance than pyrochars for its higher available K content. Risk of potentially toxic elements contamination by the sludge was efficiently reduced in either hydrochars or pyrochars, except the high leaching potential of Zn in pyrochars. Overall, in addition to the advantages of the hydrothermal carbonization process as energy saving and value-added liquid by-products, the hydrochars derived from sludge, with sufficient and balanced nutrients and limited PTEs pollution risk, can be a feasible and value-added material as soil amendment.},
	urldate = {2021-06-04},
	journal = {Chemosphere},
	author = {Fei, Ying-heng and Zhao, Dan and Liu, Ye and Zhang, Weihua and Tang, Yuan-yuan and Huang, Xuexia and Wu, Qihang and Wang, Yue-xing and Xiao, Tangfu and Liu, Chengshuai},
	month = dec,
	year = {2019},
	pmid = {31545182},
	note = {Publisher: Elsevier Ltd},
	keywords = {Sludge, Hydrochar, Agricultural applications, Nutrients, Potentially toxic elements},
	pages = {124841},
	file = {fei2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\4JYN3DZT\\fei2019.pdf:application/pdf},
}

@article{zhang2019,
	title = {Functions of phosphorus additives on immobilizing heavy metal cadmium in the char through pyrolysis of contaminated biomass},
	volume = {144},
	issn = {01652370},
	doi = {10.1016/j.jaap.2019.104721},
	abstract = {Pyrolysis was applied for post treatment of two crop residues contaminated by heavy metal cadmium (Cd) after biosorption, with phosphorus additives used for enhancing the recovery and stability of Cd in the char. The results indicate that phosphoric acid (PA) can increase the Cd recovery in char to a level of {\textgreater} 90\% at the PA dosage of 71–143 mg/g biomass, when the plant residues were pyrolyzed at 350 °C. The pyrolysis with phosphorus additives reduced the bioavailable Cd to {\textless} 3.5\% of total Cd, but the influence on the leachable Cd was varied with specific additive and crop species. The sequential extraction test indicated that phosphorus additives helped Cd in the char be transformed into more stable species. The scanning electron microscopy - energy-dispersive X-ray spectroscopy (SEM-EDS), X-ray diffraction (XRD) and infrared spectroscopy (IR) analyses indicated that immobilization of Cd in the char was related to formation of Cd phosphates and their linkage to carbon matrix of chars.},
