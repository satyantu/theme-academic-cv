	file = {li2020d.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\UTGBJXL3\\li2020d.pdf:application/pdf},
}

@article{weng2020,
	title = {Improving supercritical water gasification of sludge by oil palm empty fruit bunch addition: {Promotion} of syngas production and heavy metal stabilization},
