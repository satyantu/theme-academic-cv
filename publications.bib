	keywords = {Corncob addition proportion, Potential energy recovery, Products distribution, Pyrolysis temperature, Sludge activated carbon},
	pages = {175--183},
	file = {zhou2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\EIHDE67H\\zhou2019.pdf:application/pdf},
}

@article{gu2019,
