	file = {Zhang et al. - 2015 - Multiscale visualization of the structural and cha.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\UI8M2WQ4\\Zhang et al. - 2015 - Multiscale visualization of the structural and cha.pdf:application/pdf},
