	pages = {366--378},
	file = {krounbi2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\SBPBXRUF\\krounbi2019.pdf:application/pdf},
}

@article{cui2019,
	title = {Transformation of {Phosphorus} in {Wetland} {Biomass} during {Pyrolysis} and {Hydrothermal} {Treatment}},
