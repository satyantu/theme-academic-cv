@incollection{malairajan2021,
	title = {Management of {Phosphate} in {Domestic} {Wastewater} {Treatment} {Plants}},
	url = {https://link.springer.com.remotexs.ntu.edu.sg/chapter/10.1007/978-3-030-77795-1_3},
	abstract = {Phosphorus, being an essential nutrient, accounts for about 0.8-1.1\% of the total body weight and plays a major role in cellular metabolic activities. Phosphorus from phosphate rock is widely employed in the food and feed, detergent, fertilizer, and other chemical industries. The elimination of phosphorus from the environment to aquatic ecosystems occurs by point and non-point sources. The loading of phosphate from sewage causes phosphate enrichment in the aquatic ecosystem leading to eutrophication. In the aquatic ecosystem, many directives and regulations have been put forth worldwide targeting phosphate removal from wastewater to reduce eutrophication. Phosphate removal could be achieved by physical, chemical, and biological methods. The physical and chemical methods require high energy and chemical inputs to produce a huge volume of sludge, making it difficult for disposal. Biological phosphate removal is advantageous over the other phosphate removal methods due to its simplicity and economic viability and therefore adopted widely in wastewater treatment plants. Enhanced biological phosphate removal treatment plants remove phosphate by enriching phosphate accumulating organisms that store phosphate intra-cellularly by alternating between aerobic and anaerobic cycles. Phosphate, being a non-renewable resource, the present scenario at Municipal Wastewater treatment plants involves phosphate removal targeting its recovery in a usable form for application in various sectors.},
	urldate = {2021-10-05},
	publisher = {Springer, Cham},
	author = {Malairajan, Sumathi and Namasivayam, Vasudevan},
	year = {2021},
