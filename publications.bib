	year = {2020},
	note = {Publisher: Elsevier B.V.},
	keywords = {Syngas, Supercritical water, Sludge, Co-gasification, Heavy metal, Empty fruit bunch},
	pages = {293--298},
	file = {weng2020.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\P56U269D\\weng2020.pdf:application/pdf},
}

@article{zhang2020e,
