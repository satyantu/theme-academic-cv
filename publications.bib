	journal = {Journal of Analytical and Applied Pyrolysis},
	author = {Zielińska, Anna and Oleszczuk, Patryk and Charmas, Barbara and Skubiszewska-Zięba, Jadwiga and Pasieczna-Patkowska, Sylwia},
