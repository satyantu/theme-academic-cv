	author = {Meng, Xiangdong and Liu, Xiaoji and Huang, Qunxing and Gao, Huaping and Tay, Kangrou and Yan, Jianhua},
	month = may,
	year = {2019},
	pmid = {31088676},
	note = {Publisher: Elsevier Ltd},
	keywords = {Phosphorus recovery, Struvite, Cation exchange bed, Low-temperature combustion sludge ash, Macroscopic parametric equation Thomas model},
	pages = {84--93},
	file = {meng2019.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\82I7VIHX\\meng2019.pdf:application/pdf},
