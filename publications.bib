	keywords = {Phosphorus recovery, Biochar, Wastewater, Phosphate adsorption, P recycling, PH buffering},
	pages = {155--165},
	file = {shepherd2016.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\WWYGV9E8\\shepherd2016.pdf:application/pdf},
