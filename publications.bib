	journal = {Fuel},
	author = {Chen, Zongqi and Yang, Shoumeng and Zhang, Lihui and Duan, Feng},
	month = sep,
	year = {2023},
	keywords = {Sewage sludge, Phosphorus, Degradation solvent extraction, Organic matters, Paper-making sludge},
