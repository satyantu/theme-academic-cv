	author = {Niu, Xin and Shen, Laihong},
	month = mar,
	year = {2018},
	note = {Publisher: Elsevier B.V.},
	keywords = {Sewage sludge, Phosphorus, Available phosphorus, Chemical looping combustion, Sludge ash},
	pages = {621--630},
	file = {niu2018.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\TMWAQAG4\\niu2018.pdf:application/pdf},
}

