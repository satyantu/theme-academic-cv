	keywords = {Anaerobic digestion, review, Aqueous phase, Bioelectrochemical systems (BESs), Hydrothermal carbonization and liquefaction, Microalgae cultivation, Post hydrothermal wastewater (PHWW)},
	pages = {142383},
	file = {leng2020a.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\59PEMNB4\\leng2020a.pdf:application/pdf},
