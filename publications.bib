@article{yao2013,
	title = {Engineered {Biochar} {Reclaiming} {Phosphate} from {Aqueous} {Solutions}: {Mechanisms} and {Potential} {Application} as a {Slow}-{Release} {Fertilizer}},
	volume = {47},
	issn = {0013-936X},
