	note = {Publisher: MDPI AG},
	keywords = {Thermodynamics, Aerodynamic levitation, High entropy oxides, Lasermelting, Melting, Phase transition, Rare earth oxides},
	pages = {3141},
	file = {ushakov2020.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\HH52NMCV\\ushakov2020.pdf:application/pdf},
}

@article{carollo2019,
