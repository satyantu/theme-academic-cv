	year = {2017},
	keywords = {Anaerobic digestion, Methane potential, Modeling, ADM1, Kinetics, Spectroscopy},
	pages = {27--35},
	file = {Charnier2017.pdf:C\:\\Users\\Satyabrat\\Zotero\\storage\\H49YS6G6\\Charnier2017.pdf:application/pdf},
}

@article{Yadav2019,
