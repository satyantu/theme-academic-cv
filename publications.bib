	keywords = {Aerobic granular sludge, Granulation process, Low superficial gas velocity, Performance, SBR},
	pages = {9058--9064},
	file = {ScienceDirect Snapshot:C\:\\Users\\Satyabrat\\Zotero\\storage\\IV5H6MDY\\S0960852410012356.html:text/html},
}

