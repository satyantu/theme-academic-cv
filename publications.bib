	urldate = {2020-12-15},
	journal = {medRxiv},
	author = {Jorgensen, Alissa Udi and Gamst, Jesper and Hansen, Line Visby and Knudsen, Ida Ingeborg Hogh and Jensen, Soren Krohn Skovgaard},
	month = jul,
