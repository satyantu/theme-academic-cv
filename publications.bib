	note = {Publisher: Elsevier},
	keywords = {Organic matter, Sequential extraction, Acid and basic neutralization capacity, French constructed wetlands, Sludge deposit, Trace elements release},
	pages = {544--553},
}

@article{lee2018,
	title = {Phosphorous recovery from sewage sludge using calcium silicate hydrates},
