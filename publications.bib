	urldate = {2020-12-15},
	journal = {medRxiv},
	author = {Iglesias, Néstor Gabriel and Gebhard, Leopoldo Germán and Carballeda, Juan Manuel and Aiello, Ignacio and Recalde, Emiliano and Terny, Gabriel and Ambrosolio, Silvina and L’Arco, Gabriela and Konfino, Jonatan and Brardinelli, Juan Ignacio},
